{
  "name": "template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "npm run lint:code && npm run lint:styles",
    "lint-fix": "npm run lint:code:fix && npm run lint:styles:fix",
    "lint:code": "eslint . --report-unused-disable-directives --max-warnings 0",
    "lint:code:fix": "eslint . --report-unused-disable-directives --max-warnings 0 --fix",
    "lint:styles": "stylelint 'src/**/*.{css,module.css}'",
    "lint:styles:fix": "stylelint 'src/**/*.{css,module.css}' --fix",
    "preview": "vite preview",
    "format": "prettier --write '**/*.{js,jsx,ts,tsx,json,css,scss,md}'",
    "format:check": "prettier --check '**/*.{js,jsx,ts,tsx,json,css,scss,md}'",
    "format:fix": "prettier --write '**/*.{js,jsx,ts,tsx,json,css,scss,md}'"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.5.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unicorn": "^56.0.1",
    "globals": "^15.14.0",
    "prettier": "3.4.2",
    "stylelint": "^16.14.1",
    "stylelint-config-standard": "^37.0.0",
    "stylelint-order": "^6.0.4",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.22.0",
    "vite": "^6.0.5"
  }
}
